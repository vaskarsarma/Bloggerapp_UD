<!-- Recent blogs Widget Well -->
{{#Compare topvisit.count ">" 0}}
<div class="list-group">
  <h4 class="list-group-item active">Top Visit</h4>
    {{#each topvisit.result}}
        <a class="list-group-item" href="/viewblog/showdetails/{{this.blogid}}">{{this.blogtopic}}</a> 
    {{/each}}
</div>
{{/Compare}}