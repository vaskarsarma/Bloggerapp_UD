<!-- Recent blogs Widget Well -->
{{#Compare mostrecentblogs.count ">" 0}}
<div class="list-group">
  <h4 class="list-group-item active">Recent Blogs</h4>
    {{#each mostrecentblogs.result}}
        <a class="list-group-item" href="/viewblog/showdetails/{{this._id}}">{{this.topic}}</a> 
    {{/each}}
</div>
{{/Compare}}